{% extends 'base.html.twig' %}

{% block header %}
    {% include 'Parameter/header.html.twig' %}
{% endblock %}
{% block stylesheets %}
    <style>
        section#header{
            background-image: url({{ asset('bundles/house/img/header-land.png') }});
            max-height: 190px;
            min-height: 150px;
        }
    </style>
{% endblock %}
{% block main %}

    {% include 'Parameter/allproducts.html.twig' %}

